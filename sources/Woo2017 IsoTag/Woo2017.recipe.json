"template" : {
  "data" : {
    "$.data[*].acc" : {
      "acc" : "$[0].acc",
      "peptide_start" : "$[0].peptide_start",
      "peptide_end" : "$[0].peptide_end",
      "sites" : "$[0].sites",
      "composition" : "$[0].(join(@.composition,';'))"
    }
  },
  "metadata": {
    "msdata-version" : "1.3",
    "title" : "O-GlcNAc T-Cells Woo2018",
    "mimetype" : "application/json+msdata",
