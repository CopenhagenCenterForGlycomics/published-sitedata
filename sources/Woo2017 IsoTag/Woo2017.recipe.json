    "HexNAc(5)Hex(5)NeuAzBr2OH(1)NeuAc(1)" : "5xHexNAc5xHex2xSia",
    "HexNAc(5)Hex(5)NeuAzBr2OH(1)NeuAz(1)" : "5xHexNAc5xHex2xSia",
    "HexNAc(5)Hex(5)NeuAzBr2OH(2)" : "5xHexNAc5xHex2xSia"
  }
