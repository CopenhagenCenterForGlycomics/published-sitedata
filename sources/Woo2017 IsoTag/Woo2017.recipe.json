    "HexNAc(4)Hex(5)Fuc(1)NeuAzBr2OH(1)" : "4xHexNAc5xHex1xdHex1xSia",
    "HexNAc(4)Hex(5)Fuc(1)NeuAzBr2OH(2)" : "4xHexNAc5xHex1xdHex2xSia",
    "HexNAc(4)Hex(5)NeuAzBr2OH(1)" : "4xHexNAc5xHex1xSia",
